{
    "name": "@basmilius/vite-vue-preset",
    "version": "1.8.0",
    "description": "My personal presets for use in Vite + Vue projects.",
    "type": "module",
    "license": "MIT",
    "funding": "https://github.com/sponsors/basmilius",
    "author": {
        "name": "Bas Milius",
        "email": "bas@mili.us",
        "url": "https://bas.dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/basmilius/vite-vue-preset.git"
    },
    "keywords": [
        "vite",
        "vue",
        "preset",
        "plugin"
    ],
    "scripts": {
        "build": "tsc && esbuild src/index.ts --bundle --minify --external:* --platform=node --target=esnext --format=esm --outfile=dist/index.js"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "files": [
        "dist",
        "CODEOWNERS",
        "LICENSE",
        "README.md"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.ts",
    "sideEffects": false,
    "dependencies": {
        "@laynezh/vite-plugin-lib-assets": "^0.5.26",
        "css-class-generator": "^2.0.0",
        "vite-css-modules": "^1.6.0",
        "vite-plugin-dts": "^4.3.0"
    },
    "devDependencies": {
        "@types/node": "^22.10.2",
        "esbuild": "^0.24.0",
        "typescript": "^5.7.2",
        "vite": "~5.4.11"
    }
}
