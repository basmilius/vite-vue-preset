{
    "name": "@basmilius/vite-vue-preset",
    "version": "3.0.0",
    "description": "Commonly used presets used in Vite + Vue setups.",
    "type": "module",
    "license": "MIT",
    "funding": "https://github.com/sponsors/basmilius",
    "author": {
        "name": "Bas Milius",
        "email": "bas@mili.us",
        "url": "https://bas.dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/basmilius/vite-vue-preset.git"
    },
    "keywords": [
        "vite",
        "vue",
        "preset",
        "plugin"
    ],
    "scripts": {
        "build": "bun --bun build.ts"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "files": [
        "dist",
        "src",
        "CODEOWNERS",
        "LICENSE",
        "README.md"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.ts",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./oxc": {
            "types": "./dist/oxc.d.ts",
            "default": "./dist/oxc.js"
        }
    },
    "dependencies": {
        "@laynezh/vite-plugin-lib-assets": "^1.1.0",
        "change-case": "^5.4.4",
        "css-class-generator": "^2.0.0",
        "lightningcss": "^1.29.3",
        "oxc-transform": "^0.65.0",
        "vite-css-modules": "^1.8.5",
        "vite-plugin-dts": "^4.5.3"
    },
    "devDependencies": {
        "@types/bun": "^1.2.10",
        "vite": "^6.3.2"
    }
}
