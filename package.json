{
    "name": "@basmilius/vite-vue-preset",
    "version": "1.1.0",
    "description": "My personal presets for use in Vite + Vue projects.",
    "type": "module",
    "license": "MIT",
    "funding": "https://github.com/sponsors/basmilius",
    "repository": {
        "type": "git",
