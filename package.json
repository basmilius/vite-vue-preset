{
    "name": "@basmilius/vite-vue-preset",
    "version": "1.0.0",
    "description": "My personal presets for use in Vite + Vue projects.",
    "type": "module",
    "license": "MIT",
    "funding": "https://github.com/sponsors/basmilius",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/basmilius/vite-vue-preset.git"
    },
    "keywords": [
        "vite",
        "vue",
        "preset",
        "plugin"
    ],
    "scripts": {
        "build": "esbuild index.ts --bundle --external:generic-names --external:lightningcss --external:picomatch --external:postcss --external:postcss* --platform=node --target=esnext --format=esm --outfile=index.js"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "main": "index.js",
    "sideEffects": false,
    "dependencies": {
        "css-class-generator": "^2.0.0",
        "vite-css-modules": "^1.5.2"
    },
    "devDependencies": {
        "esbuild": "^0.24.0",
        "vite": "^5.4.10"
    }
}
