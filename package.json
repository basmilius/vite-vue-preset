{
    "name": "@basmilius/vite-vue-preset",
    "version": "2.1.0",
    "description": "Commonly used presets used in Vite + Vue setups.",
    "type": "module",
    "license": "MIT",
    "funding": "https://github.com/sponsors/basmilius",
    "author": {
        "name": "Bas Milius",
        "email": "bas@mili.us",
        "url": "https://bas.dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/basmilius/vite-vue-preset.git"
    },
    "keywords": [
        "vite",
        "vue",
        "preset",
        "plugin"
    ],
    "scripts": {
        "build": "tsc && esbuild src/index.ts --bundle --minify --packages=external --platform=node --target=esnext --format=esm --outfile=dist/index.js"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "files": [
        "dist",
        "src",
        "CODEOWNERS",
        "LICENSE",
        "README.md"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.ts",
    "sideEffects": false,
    "dependencies": {
        "@laynezh/vite-plugin-lib-assets": "^1.1.0",
        "change-case": "^5.4.4",
        "css-class-generator": "^2.0.0",
        "vite-css-modules": "^1.8.4",
        "vite-plugin-dts": "^4.5.3"
    },
    "devDependencies": {
        "@types/node": "^22.13.10",
        "esbuild": "^0.25.1",
        "typescript": "^5.8.2",
        "vite": "^6.2.2"
    }
}
